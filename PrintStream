public class Test {
	public static void main(String[] args) {
		try {
			PrintStream ps=new PrintStream("a.txt");
			ps.print("HelloWorld中国");
		} catch (FileNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}
}
